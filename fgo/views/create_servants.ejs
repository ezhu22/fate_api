<head>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>
<body>
  <div class='col'>
    <div class='row justify-content-center'>
        <div class='col'>
          <h1>Add Servant:</h1>
          <form action="/api/v1/servants" method="POST" id='servants_form'>
            <div class='form-group'>
              <div class='row text-center'>
                <div class='col'>
                  <div class='input-group mb-3'>
                    <div class='input-group-prepend'>
                      <span  class='input-group-text font-weight-bold'>Name:</span>
                    </div>
                    <input class='form-control' type='text' name='name'>
                  </div>
                  <div class='row'>
                    <div class='input-group mb-3 col-4'>
                      <div class='input-group-prepend'>
                        <span class='input-group-text font-weight-bold'>Class:</span>
                      </div>
                      <select name='class' form='servants_form' class='form-control'>
                        <option value=''></option>
                        <option value='Saber'>Saber</option>
                        <option value='Archer'>Archer</option>
                        <option value='Lancer'>Lancer</option>
                        <option value='Rider'>Rider</option>
                        <option value='Caster'>Caster</option>
                        <option value='Assassin'>Assassin</option>
                        <option value='Berserker'>Berserker</option>
                        <option value='Avenger'>Avenger</option>
                        <option value='Alter Ego'>Alter Ego</option>
                        <option value='Foreigner'>Foreigner</option>
                        <option value='Moon Cancer'>Moon Cancer</option>
                        <option value='Shielder'>Shielder</option>
                      </select>
                    </div>
                    <div class='input-group mb-3 col-2'>
                      <div class='input-group-prepend'>
                        <span class='input-group-text font-weight-bold'>ID:</span>
                      </div>
                      <input type='number' class='form-control' name='id'>
                    </div>
                    <div class='input-group mb-3 col-3'>
                      <div class='input-group-prepend'>
                        <span class='input-group-text font-weight-bold'>Rarity:</span>
                      </div>
                      <input type='number' class='form-control' name='rarity'>
                    </div>
                    <div class='input-group mb-3 col-3'>
                      <div class='input-group-prepend'>
                        <span class='input-group-text font-weight-bold'>Cost:</span>
                      </div>
                      <input type='number' class='form-control' name='cost'>
                    </div>
                  </div>
                    <div class='row'>
                        <div class='input-group mb-3 col-4'>
                          <div class='input-group-prepend'>
                            <span  class='input-group-text font-weight-bold'>Gender:</span>
                          </div>
                          <select name='gender' form='servants_form' class='form-control'>
                              <option value=''></option>
                              <option value='Male'>Male</option>
                              <option value='Female'>Female</option>
                              <option value='N/A'>N/A</option>
                            </select>
                        </div>
                      <div class='input-group mb-3 col-4'>
                        <div class='input-group-prepend'>
                          <span  class='input-group-text font-weight-bold'>Attribute:</span>
                        </div>
                        <input class='form-control' type='text' name='attribute'>
                      </div>
                      <div class='input-group mb-3 col-4'>
                        <div class='input-group-prepend'>
                          <span  class='input-group-text font-weight-bold'>Alignment:</span>
                        </div>
                          <select name='alignment' form='servants_form' class='form-control'>
                            <option value=''></option>
                            <option value='Lawful Good'>Lawful Good</option>
                            <option value='Neutral Good'>Neutral Good</option>
                            <option value='Chaotic Good'>Chaotic Good</option>
                            <option value='Lawful Neutral'>Lawful Neutral</option>
                            <option value='Neutral'>Neutral</option>
                            <option value='Chaotic Neutral'>Chaotic Neutral</option>
                            <option value='Lawful Evil'>Lawful Evil</option>
                            <option value='Neutral Evil'>Neutral Evil</option>
                            <option value='Chaotic Evil'>Chaotic Evil</option>
                          </select>
                      </div>
                    </div>
                      <div class='input-group mb-3'>
                        <div class='input-group-prepend'>
                          <span  class='input-group-text font-weight-bold'>Ascension 1 Image Link:</span>
                        </div>
                        <input type='hidden' name='ascension_stage' value='1'>
                        <input class='form-control' type='text' name='ascension_image_link'>
                      </div>
                      <div class='input-group mb-3'>
                        <div class='input-group-prepend'>
                          <span  class='input-group-text font-weight-bold'>Ascension 2 Image Link:</span>
                        </div>
                        <input type='hidden' name='ascension_stage' value='2'>
                        <input class='form-control' type='text' name='ascension_image_link'>
                      </div>
                      <div class='input-group mb-3'>
                        <div class='input-group-prepend'>
                          <span  class='input-group-text font-weight-bold'>Ascension 3 Image Link:</span>
                        </div>
                        <input type='hidden' name='ascension_stage' value='3'>
                        <input class='form-control' type='text' name='ascension_image_link'>
                      </div>
                      <div class='input-group mb-3'>
                        <div class='input-group-prepend'>
                          <span  class='input-group-text font-weight-bold'>Ascension 4 Image Link:</span>
                        </div>
                        <input type='hidden' name='ascension_stage' value='4'>
                        <input class='form-control' type='text' name='ascension_image_link'>
                      </div>
                    </div>
                <div class='row text-center'>
                  <div class='col'>
                    <table class='table text-center'>
                        <thead>
                            <tr>
                                <th>Deck</th>
                                <th>Card Type</th>
                                <th>Hit Count</th>
                            </tr>
                        </thead>
                        <tr>
                            <th scope='row'>
                              <select class='form-control font-weight-bold' name='deck' form='servants_form'>
                                <option value=''></option>
                                <option value='buster'>Buster</option>
                                <option value='arts'>Arts</option>
                                <option value='quick'>Quick</option>
                              </select>
                            </th>
                            <th>Buster<input type='hidden' name='hit_count_card_type' value='buster'></th>
                            <th><input class='form-control' type='number' name='hit_count_hits' form='servants_form'></th>
                        </tr>
                        <tr>
                            <th scope='row'>
                              <select class='form-control font-weight-bold' name='deck' form='servants_form'>
                                <option value=''></option>
                                <option value='buster'>Buster</option>
                                <option value='arts'>Arts</option>
                                <option value='quick'>Quick</option>
                              </select>
                            </th>
                            <th>Arts<input type='hidden' name='hit_count_card_type' value='arts'></th>
                            <th><input class='form-control' type='number' name='hit_count_hits' form='servants_form'></th>
                        </tr>
                        <tr>
                          <th scope='row'>
                            <select class='form-control font-weight-bold' name='deck' form='servants_form'>
                              <option value=''></option>  
                              <option value='buster'>Buster</option>
                                <option value='arts'>Arts</option>
                                <option value='quick'>Quick</option>
                            </select>
                          </th>
                          <th>Quick<input type='hidden' name='hit_count_card_type' value='quick'></th>
                          <th><input class='form-control' type='number' name='hit_count_hits' form='servants_form'></th>
                        </tr>
                        <tr>
                          <th scope='row'>
                            <select class='form-control font-weight-bold' name='deck' form='servants_form'>
                              <option value=''></option>
                              <option value='buster'>Buster</option>
                              <option value='arts'>Arts</option>
                              <option value='quick'>Quick</option>
                            </select>
                          </th>
                          <th>Extra<input type='hidden' name='hit_count_card_type' value='extra'></th>
                          <th><input class='form-control' type='number' name='hit_count_hits' form='servants_form'></th>
                        </tr>
                        <tr>
                          <th scope='row'>
                            <select class='form-control font-weight-bold' name='deck' form='servants_form'>
                              <option value=''></option>  
                              <option value='buster'>Buster</option>
                                <option value='arts'>Arts</option>
                                <option value='quick'>Quick</option>
                            </select>
                          </th>
                        </tr>
                            
                    </table>
                  </div>
                </div>
                <div class='row text-center'>
                  <div class='col ml-3'>
                    <div class='row'>
                      <div class='input-group mb-3 col-4'>
                        <div class='input-group-prepend'>
                            <span  class='input-group-text font-weight-bold'>Base Attack:</span>
                        </div>
                            <input class='form-control' type='number' name='base_attack'>
